{
    "name": "i2ui",
    "version": "2.1.2",
    "description": "i2ui - Intuitively Understandable User Interface",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "devDependencies": {
        "@types/jest": "^24.0.20",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "cross-env": "^7.0.3",
        "jest": "^24.9.0",
        "prepend-file": "^1.3.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "rollup": "^3.20.2",
        "rollup-obfuscator": "^3.0.2",
        "rollup-plugin-scss": "^1.0.2",
        "ts-jest": "^24.1.0",
        "tslib": "^2.5.0",
        "typescript": "^4.8.2"
    },
    "peerDependencies": {
        "react": ">=16",
        "react-dom": ">=16"
    },
    "scripts": {
