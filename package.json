        "@typescript-eslint/eslint-plugin": "^4.26.0",
        "@typescript-eslint/parser": "^4.26.0",
        "concurrently": "^4.1.1",
        "eslint": "^7.27.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-config-varp": "^0.3.1",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-import-resolver-typescript": "^2.4.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "jest": "^24.9.0",
        "prepend-file": "^1.3.1",
        "prettier": "^2.3.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "rollup": "^1.19.4",
        "rollup-plugin-scss": "^1.0.2",
        "rollup-plugin-typescript2": "^0.22.1",
        "rollup-plugin-uglify": "^6.0.3",
        "stylelint": "^14.11.0",
        "stylelint-config-standard-scss": "^5.0.0",
        "stylelint-config-varp": "^0.4.3",
        "stylelint-order": "^5.0.0",
        "stylelint-scss": "^4.3.0",
        "stylelint-selector-bem-pattern": "^2.1.1",
        "ts-jest": "^24.1.0",
        "tslib": "^2.5.0",
        "typescript": "^4.8.2"
    },
    "peerDependencies": {
        "react": ">=16",
        "react-dom": ">=16"
    },
    "scripts": {
        "build": "rollup -c",
        "build-watch": "rollup -c -w",
        "install-all": "npm i && cd example && npm i",
        "start-e": "cd ucase && npm start",
        "build-e": "npm run build && cd sample && npm run build",
        "dev": "concurrently --kill-others \"npm run build-watch\" \"npm run start-example\"",
        "test": "jest",
        "prepublishOnly": "npm run test && npm run lint",
        "lint:old": "eslint ./src/**/*.ts ./src/**/*.tsx ./example/src/**/*.ts ./example/src/**/*.tsx",
        "lint": "stylelint \"**/*.scss\" && eslint .",
        "lint:fix": "stylelint \"**/*.scss\" --fix && eslint . --fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/berezh/i2ui.git"
    },
    "keywords": [
        "tag-cloud",
        "tagcloud",
        "react-tag-cloud",
        "word-cloud",
        "wordcloud",
        "react",
        "number",
        "number-group",
        "emphase"
    ],
