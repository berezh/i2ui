{
    "compilerOptions": {
        "declaration": true,
        "strict": true,
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "noUnusedParameters": true,
        "strictNullChecks": true,
        "moduleResolution": "node",
        "noImplicitAny": true,
        "outDir": "./dist",
        "target": "es5",
        "jsx": "react",
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}
